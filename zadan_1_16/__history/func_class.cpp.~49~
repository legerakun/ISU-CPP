#ifndef FUNC
#define FUNC
#include<conio.h>
#include<stdio.h>
#include "desc.h"

Complex::Complex(double a, double b){
	real = a;
	imagine = b;
}

double ComplexModule() {
	double module = 0;
    module = sqrt(real*real + imagine*imagine)
}

Complex::Argument() {

}

double Complex::outA(){
	return real;
}

double Complex::outB(){
	return imagine;
}
#endif FUNC
